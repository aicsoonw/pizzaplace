!/^[\sа-яё]{1,50}$/i.test('Для имен');                                                                  //имена

!/^[\sа-яё.,0-9]{1,150}$/i.test('610000 Россия, Кировская обл., г. Киров, ул. Пушкина д.228 кв.135');   //адреса

!/^[0-9]{10}$/i.test('9992251234');                                                                     //номера телефонов